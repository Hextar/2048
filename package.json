  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
